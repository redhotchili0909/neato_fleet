<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>neato_fleet</name>
  <version>0.0.0</version>
  <description>ROS 2 package for coordinating a fleet of Neato robots: goal management, command playback/recording, and RVO-based collision avoidance.</description>
  <maintainer email="dhvanshah15@gmail.com">root</maintainer>
  <license>MIT</license>

  <depend>rclpy</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
